{
  "name": "gatsby-starter-blog",
  "private": true,
  "description": "A starter for a blog powered by Gatsby and Markdown",
  "version": "0.1.0",
  "author": "@tsaristbomba",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "cross-env GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "restart": "gatsby clean && gatsby develop",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@react-icons/all-files": "^4.1.0",
    "autoprefixer": "^10.2.6",
    "babel-plugin-styled-components": "^1.13.2",
    "cross-env": "^7.0.3",  
    "gatsby": "^3.9.1",
    "gatsby-plugin-feed": "^3.9.0",
    "gatsby-plugin-google-analytics": "^3.9.0",
    "gatsby-plugin-image": "^1.9.0",
    "gatsby-plugin-manifest": "^3.9.0",
    "gatsby-plugin-netlify-cms": "^5.9.0",
    "gatsby-plugin-offline": "^4.9.0",
    "gatsby-plugin-postcss": "^4.9.0",
    "gatsby-plugin-preact": "^5.9.0",
    "gatsby-plugin-react-helmet": "^4.9.0",
    "gatsby-plugin-sharp": "^3.9.0",
    "gatsby-plugin-styled-components": "^4.9.0",
    "gatsby-remark-copy-linked-files": "^4.6.0",
    "gatsby-remark-images": "^5.6.0",
    "gatsby-remark-prismjs": "^5.6.0",
    "gatsby-remark-responsive-iframe": "^4.6.0",
    "gatsby-remark-smartypants": "^4.6.0",
    "gatsby-source-filesystem": "^3.9.0",
    "gatsby-transformer-remark": "^4.6.0",
    "gatsby-transformer-sharp": "^3.9.0",
    "netlify-cms-app": "^2.15.26",
    "postcss": "^8.3.5",
    "preact": "^10.5.14",
    "preact-render-to-string": "^5.1.19",
    "prismjs": "^1.24.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-masonry-css": "^1.0.16",
    "styled-components": "^5.3.0",
    "tailwindcss": "^2.2.4"
  },
  "devDependencies": {
    "gatsby-plugin-perf-budgets": "^0.0.18",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.24",
    "prettier": "2.3.2"
  }
}
